{
  "name": "extension-rota",
  "version": "1.0.4",
  "description": "Vibrant lands await! Explore, solve, and discover whimsical worlds!",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production node ./scripts/build.mjs",
    "dev": "nodemon",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/extpkg/extension-rota.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/extpkg/extension-rota/issues"
  },
  "homepage": "https://github.com/extpkg/extension-rota#readme",
  "devDependencies": {
    "@chialab/esbuild-plugin-meta-url": "^0.17.7",
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@extpkg/packager": "^1.0.2",
    "@extpkg/types-console": "^1.0.1",
    "@extpkg/types-runtime": "^1.2.2",
    "@extpkg/types-tabs": "^1.1.0",
    "@extpkg/types-websessions": "^1.0.3",
    "@extpkg/types-webviews": "^1.0.2",
    "@extpkg/types-windows": "^1.2.1",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint"
  }
}
